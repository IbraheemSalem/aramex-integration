{
    "name": "ibraheem/aramex-integration",
    "description": "Complete Laravel package for Aramex shipping integration with multi-merchant support, billing, SMS notifications, and dashboard",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ibraheem",
            "email": "ibraheem@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/queue": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0",
        "dompdf/dompdf": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Ibraheem\\AramexIntegration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ibraheem\\AramexIntegration\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ibraheem\\AramexIntegration\\AramexIntegrationServiceProvider"
            ],
            "aliases": {
                "AramexIntegration": "Ibraheem\\AramexIntegration\\Facades\\AramexIntegration"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

